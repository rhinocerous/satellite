<%
  if(typeof satellite == 'undefined')
  {
    console.log("WARNING: no satellite base located");
    satellite = {
      scopes:[]
      , config: sails.config.satellite
    };
  }
%>

<!DOCTYPE html><!--[if IE 9 ]>
<html class="ie9" lang="en"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html lang="en" class="no-js"><!--<![endif]-->
<head>
  <meta charset="utf-8">
  <title><%=satellite.config.defaults.settings.headerMain %></title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Place favicon.ico and apple-touch-icon.png in the root directory-->
  <link rel="stylesheet" href="/styles/naut/main.css">
  <link rel="stylesheet" href="/styles/naut/vendor.css">
  <link rel='stylesheet' href='/styles/naut/textAngular.css'>
  <link rel='stylesheet' href='/styles/angular-toastr.css'>
  <script src="/js/naut/vendor/modernizr.js"></script>

  <style type="text/css">

  </style>
  <!-- GMaps api-->
  <!--<script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=true"></script>-->
</head>

<body data-ng-app="satelliteApp">

<div id="root" data-ng-controller="appController as app" ng-class="{'aside-offscreen layout-dock': !app.sidebarActive}" ng-cloak="">
  <!-- Main container-->
  <div class="app-container">
    <!-- top navbar-->
    <header class="bg-info">
      <!-- START Top Navbar-->
      <nav role="navigation" class="navbar topnavbar">

        <!-- START navbar header-->
        <div class="navbar-header">

          <a href="/admin#/" class="navbar-brand">
            <%=satellite.config.defaults.settings.headerMain %>
          </a>

          <!-- Mobile buttons-->
          <div class="mobile-toggles">
            <a href="" class="sidebar-toggle" data-ng-click="app.toggleSidebar()">
              <em class="fa fa-navicon"></em>
            </a>
            <!--<a href="#nav-collapse" data-toggle="collapse" class="menu-toggle hidden-material">-->
              <!--<em class="fa fa-ellipsis-v fa-fw"></em>-->
            <!--</a>-->
          </div>
        </div>
        <!-- END navbar header-->
        <!-- START Nav wrapper-->
        <div id="nav-collapse" class="nav-wrapper collapse navbar-collapse">
          <!-- START Left navbar-->
          <ul class="nav navbar-nav hidden-material">
            <li>
              <a href="" class="hidden-xs sidebar-toggle-off" data-ng-click="app.toggleSidebar()">
                <em class="fa fa-navicon"></em>
              </a>
            </li>

          </ul>
          <!-- END Left navbar-->

        </div>
        <!-- END Nav wrapper-->


      </nav>
      <!-- END Top Navbar-->
    </header>
    <!-- Sidebar-->
    <aside class="bg-white">
      <!-- START Sidebar-->
      <div class="sidebar-wrapper">
        <div data-ui-sidebar="" class="sidebar">
          <div class="sidebar-nav">
            <ul class="nav">
              <!-- Iterates over all sidebar items-->
              <li class="nav-heading"><span class="text-muted">Main Navigation</span></li>
              <li ng-repeat="tab in app.tabs">
                <a href="{{tab.link}}" title="{{tab.label}}" data-ripple="">
                  <em class="sidebar-item-icon fa {{tab.icon}}"></em>
                  <span>{{tab.label}}</span>
                </a>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <!-- END Sidebar-->

    </aside>
    <!-- Main-->
    <section>
      <!-- Content-->
      <div class="app">
        <div class="app-view-header">
          <%=satellite.config.defaults.settings.headerSecond %>
          <small>
            <%=satellite.config.defaults.settings.headerTagline %>
          </small>
        </div>
        <div class="row">
          <div class="container">

            <div ng-view></div>

          </div>
        </div>
      </div>
    </section>
    <!-- Page footer-->
    <footer>
      <span>
        &copy; <%=satellite.config.defaults.settings.footerCopyrightYear %>
        - <%=satellite.config.defaults.settings.footerName %>
        <!--, Powered by <a href="https://istunt.com" target="_blank">iStunt.com</a> -->
      </span>
    </footer>
  </div>

</div>


<script type="application/json" id="satellite"><%-JSON.stringify(satellite) %></script>
<script type="text/javascript"> var SATELLITE="satelliteApp";</script>


<!--SATELLITE DEPENDENCIES-->
<script src="/min/satellite.dependencies.min.js"></script>
<!--SATELLITE DEPENDENCIES END-->


